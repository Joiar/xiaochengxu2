.pro-des{font-size: 30.4688rpx;display: -webkit-box;color:#000;line-height:48.75rpx;word-break: break-all;text-overflow: ellipsis;-webkit-box-orient: vertical;-webkit-line-clamp: 2;overflow:hidden;}
.pro-price{color:red;text-overflow:ellipsis;overflow: hidden;white-space: nowrap;display: block;}
.footer{width:100%;height:120rpx;background-color:#ff621c;position: fixed;bottom:0; }
