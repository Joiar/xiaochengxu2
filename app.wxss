.navc-1{background-color:#2FB868} .navc-2{background-color:#5B9FEC} .navc-3{background-color: #FCBC28} .navc-4{background-color:#F14850 }
.pro-nav .nav .circle image{width:53rpx;height:53rpx;margin:35rpx auto;display:block;}
